Find the prime numbers or not?

PROGRAM
package Practices;

public class Primenum {

	public static void main(String[] args) {
		int n=23;
		boolean isprime=true;
		
		if(n<2)
		{
			isprime=false;
		}
		else
		{
			for(int i=2;i<n;i++) {
				if(n%i==0) {
					isprime=false;
					break;
				}
			}
			String result=isprime?"Prime":"Not a prime";
			System.out.println(n+" : "+result);
			
		}
	}
}
